{
  "name": "wedding-site",
  "version": "1.3.2",
  "private": true,
  "dependencies": {
    "auto-changelog": "^2.3.0",
    "axios": "^0.18.0",
    "babel-eslint": "9.0.0",
    "commander": "^2.20.0",
    "dotenv": "^7.0.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "micro": "^9.3.3",
    "mongoose": "5.7.5",
    "nodemon": "^1.18.11",
    "pre-commit": "^1.2.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "semver": "^6.1.1"
  },
  "scripts": {
    "reset": "npm prune && npm install",
    "dev": "concurrently \"npm run lambda-debug\" \"react-scripts start\"",
    "now-dev": "PORT=3000 react-scripts start",
    "now-build": "react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --no-watch",
    "snapshots": "npm run test -- -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "npm run stylelint --fix",
    "prettier": "prettier --write src/**/**/*.{js,json} api/**/*.{js,json}",
    "deploy": "node scripts/deploy.js",
    "deploy:debug": "node scripts/deploy.js -d",
    "lambda-debug": "now-lambda",
    "eject": "react-scripts eject",
    "version": "node scripts/version.js",
    "version:patch": "node scripts/version.js --patch",
    "version:minor": "node scripts/version.js --minor",
    "version:major": "node scripts/version.js --major",
    "changelog": "auto-changelog -p -l 8 && git add CHANGELOG.md"
  },
  "pre-commit": [
    "lint"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "23.6.0",
    "now-lambda-runner": "^3.3.0",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.8.6",
    "stylelint": "^9.10.1"
  }
}
